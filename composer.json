{
    "name": "arkcode/chat-laravel",
    "version": "1.1",
    "description": "Live chat widget for Laravel that also includes multi-user chat, group permissions, customer support chat & more.",
    "keywords": [
        "laravel",
        "laravel-package",
        "laravel chat package"
    ],
    "license": "MIT",
    "homepage": "https://github.com/Sirajunnasihin/Chat-Laravel",
    "authors": [
        {
            "name": "arkcode",
            "email": "therasoftware010@gmail.com"
        }
    ],
    "require": {
        "pusher/pusher-php-server": "*",
        "illuminate/support": "~8.0|~9.0|~10.0",
        "doctrine/dbal": "^3.7"
    },
    "require-dev": {
        "laravel/framework": "~8.0|~9.0|10.0"
    },
    "autoload": {
        "psr-4": {
            "Arkcode\\Addchat\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable" : true,
    "extra": {
        "laravel": {
            "providers": [
                "Arkcode\\Addchat\\AddchatServiceProvider"
            ]
        }
    }
}